-- -----------------------------
-- Table structure for 模型_检测标准
-- -----------------------------
-- DROP TABLE ADS_MODEL_CHECK_STANDARD;
CREATE TABLE ADS_MODEL_CHECK_STANDARD (
  ID                 VARCHAR2(64) NOT NULL,
  NAME               VARCHAR2(200) NOT NULL,
  VALUE              NUMBER NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_CHECK_STANDARD ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_CHECK_STANDARD is '模型_检测标准';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.NAME is '标准名称';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.VALUE is '检测标准上限值';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_CHECK_STANDARD.DEL_FLAG is '删除标识';

-- -----------------------------
-- Table structure for 模型_检测项目
-- -----------------------------
-- DROP TABLE ADS_MODEL_CHECK_ITEM;
CREATE TABLE ADS_MODEL_CHECK_ITEM (
  ID                 VARCHAR2(64) NOT NULL,
  STANDARD_ID       VARCHAR2(64) NOT NULL,
  NAME               VARCHAR2(200) NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_CHECK_ITEM ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_CHECK_ITEM is '模型_检测项目';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.STANDARD_ID is '检测标准id';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.NAME is '项目名称';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_CHECK_ITEM.DEL_FLAG is '删除标识';


-- -----------------------------
-- Table structure for 模型_检测对象
-- -----------------------------
-- DROP TABLE ADS_MODEL_CHECK_OBJECT;
CREATE TABLE ADS_MODEL_CHECK_OBJECT (
  ID                 VARCHAR2(64) NOT NULL,
  NAME               VARCHAR2(200) NOT NULL,
  NUMBERS            NUMBER,
  CREATE_BY           VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_CHECK_OBJECT ADD PRIMARY KEY (ID);
CREATE UNIQUE INDEX index_obj_name ON ADS_MODEL_CHECK_OBJECT(NAME);

COMMENT ON TABLE ADS_MODEL_CHECK_OBJECT is '模型_检测对象';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.NAME is '对象名称';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.NUMBERS is '抽样数量';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_CHECK_OBJECT.DEL_FLAG is '删除标识';

-- -----------------------------
-- Table structure for 模型_检测对象与检测项目映射表
-- -----------------------------
-- DROP TABLE ADS_MODEL_OBJECT_ITEM_MAPPING;
CREATE TABLE ADS_MODEL_OBJECT_ITEM_MAPPING (
  ID                 VARCHAR2(64) NOT NULL,
  OBJECT_ID         VARCHAR2(64) NOT NULL,
  ITEM_ID            VARCHAR2(64) NOT NULL,
  CREATE_BY           VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_OBJECT_ITEM_MAPPING ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_OBJECT_ITEM_MAPPING is '模型_检测对象与检测项目映射表';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.OBJECT_ID is '对象id';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.ITEM_ID is '项目id';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_OBJECT_ITEM_MAPPING.DEL_FLAG is '删除标识';

-- -----------------------------
-- Table structure for 模型_判定标准
-- -----------------------------
-- DROP TABLE ADS_MODEL_JUDGE_STANDARD;
CREATE TABLE ADS_MODEL_JUDGE_STANDARD (
  ID                 VARCHAR2(64) NOT NULL,
  NAME               VARCHAR2(200) NOT NULL,
  FILE_PATH         VARCHAR2(200),
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_JUDGE_STANDARD ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_JUDGE_STANDARD is '模型_判定标准';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.NAME is '标准名称';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.FILE_PATH is '文件路径';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_JUDGE_STANDARD.DEL_FLAG is '删除标识';

-- -----------------------------
-- Table structure for 检测模型
-- -----------------------------
-- DROP TABLE ADS_CHECK_MODEL;
CREATE TABLE ADS_CHECK_MODEL (
  ID                 VARCHAR2(64) NOT NULL,
  MODEL_NAME         VARCHAR2(32) NOT NULL,
  MONITOR_TYPE       VARCHAR2(32) NOT NULL,
  INDUSTRY           VARCHAR2(64),
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N',
  INDUSTRY_ID           VARCHAR2(64)
);

ALTER TABLE ADS_CHECK_MODEL ADD PRIMARY KEY (ID);
CREATE UNIQUE INDEX index_model_name ON ADS_CHECK_MODEL(MODEL_NAME);


COMMENT ON TABLE ADS_CHECK_MODEL is '检测模型';
COMMENT ON COLUMN  ADS_CHECK_MODEL.ID is '主键id';
COMMENT ON COLUMN  ADS_CHECK_MODEL.MODEL_NAME is '模型名称';
COMMENT ON COLUMN  ADS_CHECK_MODEL.MONITOR_TYPE is '监测类型';
COMMENT ON COLUMN  ADS_CHECK_MODEL.INDUSTRY is '行业';
COMMENT ON COLUMN  ADS_CHECK_MODEL.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_CHECK_MODEL.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_CHECK_MODEL.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_CHECK_MODEL.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_CHECK_MODEL.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_CHECK_MODEL.INDUSTRY_ID is '行业id';

-- -----------------------------
-- Table structure for 模型_种类
-- -----------------------------
-- DROP TABLE ADS_MODEL_TYPE;
CREATE TABLE ADS_MODEL_TYPE (
  ID            VARCHAR2(64) NOT NULL,
  NAME         VARCHAR2(64) NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_TYPE ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_TYPE is '模型_模型种类';
COMMENT ON COLUMN  ADS_MODEL_TYPE.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_TYPE.NAME is '种类名称';
COMMENT ON COLUMN  ADS_MODEL_TYPE.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_TYPE.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_TYPE.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_TYPE.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_TYPE.DEL_FLAG is '删除标识';

-- -----------------------------
-- Table structure for 模型_抽样环节
-- -----------------------------
-- DROP TABLE ADS_MODEL_SAMPLE_LINK;
CREATE TABLE ADS_MODEL_SAMPLE_LINK (
  ID                 VARCHAR2(64) NOT NULL,
  NAME              VARCHAR2(200) NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_SAMPLE_LINK ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_SAMPLE_LINK is '模型_抽样环节';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.NAME is '抽样环节名称';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_SAMPLE_LINK.DEL_FLAG is '删除标识';

-- -----------------------------
-- Table structure for 检测模型与属性关联映射表
-- -----------------------------
CREATE TABLE ADS_MODEL_ATTRIBUTE_MAPPING (
  ID                 VARCHAR2(64) NOT NULL,
  MODEL_ID          VARCHAR2(64) NOT NULL,
  OBJECT_ID         VARCHAR2(64) NOT NULL,
  TYPE               VARCHAR2(32),
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N'
);

ALTER TABLE ADS_MODEL_ATTRIBUTE_MAPPING ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_MODEL_ATTRIBUTE_MAPPING is '检测模型与属性关联映射表';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.ID is '主键id';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.MODEL_ID is '模型id';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.OBJECT_ID is '子表对象id（种类、牵头单位、检测对象、监测地区、抽样环节、判定标准）';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.TYPE is '关联类型（种类、检测对象、抽样环节、判定标准）';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MODEL_ATTRIBUTE_MAPPING.DEL_FLAG is '删除标识';




