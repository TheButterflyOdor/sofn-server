-- ----------------------------
-- Table structure for 回执单
-- ----------------------------
-- DROP TABLE ADS_RECIPE;
CREATE TABLE ADS_RECIPE (
  ID         		VARCHAR2(64) NOT NULL,
  ORGAN_NAME        VARCHAR2(32) NOT NULL,
  ORGAN_TASK_ID     VARCHAR2(64) NOT NULL,
  ORGAN_ID          VARCHAR2(64) NOT NULL,
  TASK_NUM          NUMBER,
  RECEIPT_CLASS     VARCHAR2(64) NOT NULL,
  RECEIPT_TIME      TIMESTAMP NOT NULL,
  REASON            VARCHAR2(64) NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200)
);

ALTER TABLE ADS_RECIPE ADD PRIMARY KEY (ID);
ALTER TABLE ADS_RECIPE ADD UNIQUE(ORGAN_ID);
CREATE INDEX SO_ORGAN_TASK_ID_IDX ON ADS_RECIPE(ORGAN_TASK_ID);
CREATE INDEX SO_RECEIPT_CLASS_IDX ON ADS_RECIPE(RECEIPT_CLASS);

COMMENT ON TABLE ADS_RECIPE is '回执单';
COMMENT ON COLUMN  ADS_RECIPE.ID is '主键id';
COMMENT ON COLUMN  ADS_RECIPE.ORGAN_NAME is '机构名称';
COMMENT ON COLUMN  ADS_RECIPE.ORGAN_TASK_ID is '机构任务KEY';
COMMENT ON COLUMN  ADS_RECIPE.ORGAN_ID is '机构KEY';
COMMENT ON COLUMN  ADS_RECIPE.TASK_NUM is '任务数';
COMMENT ON COLUMN  ADS_RECIPE.RECEIPT_CLASS is '回执单类型';
COMMENT ON COLUMN  ADS_RECIPE.RECEIPT_TIME is '上报时间';
COMMENT ON COLUMN  ADS_RECIPE.REASON is '退回原因';
COMMENT ON COLUMN  ADS_RECIPE.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_RECIPE.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_RECIPE.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_RECIPE.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_RECIPE.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_RECIPE.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_RECIPE.RESERVED_FIELD2 is '预留字段2';

-- ----------------------------
-- Table structure for 文件
-- ----------------------------
-- DROP TABLE ADS_FILE;
CREATE TABLE ADS_FILE (
  ID         		VARCHAR2(64) NOT NULL,
  FILE_NAME         VARCHAR2(128) NOT NULL,
  FILE_ADDRESS      VARCHAR2(1024) NOT NULL,
  FILE_TYPE         VARCHAR2(16) NOT NULL,
  MONITOR_TASK_ID   VARCHAR2(64) NOT NULL,
  MONITOR_TASK      VARCHAR2(64) NOT NULL,
  UPLOAD_TIME       TIMESTAMP NOT NULL,
  UPLOAD_DEPARTMENT VARCHAR2(64) NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200)
);

ALTER TABLE ADS_FILE ADD PRIMARY KEY (ID);
ALTER TABLE ADS_FILE ADD UNIQUE(MONITOR_TASK_ID);

COMMENT ON TABLE ADS_FILE is '文件';
COMMENT ON COLUMN  ADS_FILE.ID is '主键id';
COMMENT ON COLUMN  ADS_FILE.FILE_NAME is '文件名称';
COMMENT ON COLUMN  ADS_FILE.FILE_ADDRESS is '文件地址';
COMMENT ON COLUMN  ADS_FILE.FILE_TYPE is '文件类别';
COMMENT ON COLUMN  ADS_FILE.MONITOR_TASK_ID is '所属任务KEY';
COMMENT ON COLUMN  ADS_FILE.MONITOR_TASK is '所属任务';
COMMENT ON COLUMN  ADS_FILE.UPLOAD_TIME is '上传时间';
COMMENT ON COLUMN  ADS_FILE.UPLOAD_DEPARTMENT is '上传单位';
COMMENT ON COLUMN  ADS_FILE.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_FILE.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_FILE.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_FILE.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_FILE.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_FILE.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_FILE.RESERVED_FIELD2 is '预留字段2';

-- -----------------------------
-- Table structure for 监测任务
-- -----------------------------
-- DROP TABLE ADS_MONITOR_TASK;
CREATE TABLE ADS_MONITOR_TASK (
  ID             VARCHAR2(64) NOT NULL,
  TASK_NAME         VARCHAR2(32) NOT NULL,
  MONITOR_CLASS     VARCHAR2(32) NOT NULL,
  RELEASE_UNIT      VARCHAR2(32) NOT NULL,
  CHECK_MODEL       VARCHAR2(32) NOT NULL,
  YEAR              VARCHAR2(6) NOT NULL,
  BATCH             VARCHAR2(32) NOT NULL,
  SEPARATION        NUMBER NOT NULL,
  NUMBERS           NUMBER,
  START_TIME        TIMESTAMP NOT NULL,
  END_TIME          TIMESTAMP NOT NULL,
  DEADLINE          TIMESTAMP NOT NULL,
  ATTACHMENT_ADDRESS VARCHAR2(1024),
  ATTACHMENT        VARCHAR2(128),
  ATTACHMENTCODE    VARCHAR2(64),
  "COMMENT"         VARCHAR2(1024),
  PUBLISH_STATUS    NUMBER NOT NULL,
  INDUSTRY          VARCHAR2(1024),
  JUDGE_STANDARD    VARCHAR2(1024),
  SAMPLE_LINK       VARCHAR2(1024),
  CHECK_OBJECT      VARCHAR2(1024),
  CHECK_PROJECT     VARCHAR2(1024),
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200),
  LEVE              VARCHAR2(64),
  ABOLISH           VARCHAR2(1),
  ORGAN_ID          VARCHAR2(64)
);

ALTER TABLE ADS_MONITOR_TASK ADD PRIMARY KEY (ID);
CREATE INDEX SO_TASK_NAME_IDX ON ADS_MONITOR_TASK(TASK_NAME);
CREATE INDEX SO_MONITOR_CLASS_IDX ON ADS_MONITOR_TASK(MONITOR_CLASS);
CREATE INDEX SO_RELEASE_UNIT_IDX ON ADS_MONITOR_TASK(RELEASE_UNIT);

COMMENT ON TABLE ADS_MONITOR_TASK is '监测任务';
COMMENT ON COLUMN  ADS_MONITOR_TASK.ID is '主键id';
COMMENT ON COLUMN  ADS_MONITOR_TASK.TASK_NAME is '任务名称';
COMMENT ON COLUMN  ADS_MONITOR_TASK.MONITOR_CLASS is '监测类型';
COMMENT ON COLUMN  ADS_MONITOR_TASK.RELEASE_UNIT is '发布单位';
COMMENT ON COLUMN  ADS_MONITOR_TASK.CHECK_MODEL is '检测模型';
COMMENT ON COLUMN  ADS_MONITOR_TASK.YEAR is '年度';
COMMENT ON COLUMN  ADS_MONITOR_TASK.BATCH is '批次';
COMMENT ON COLUMN  ADS_MONITOR_TASK.SEPARATION is '抽检分离';
COMMENT ON COLUMN  ADS_MONITOR_TASK.NUMBERS is '单项任务数';
COMMENT ON COLUMN  ADS_MONITOR_TASK.START_TIME is '任务开始时间';
COMMENT ON COLUMN  ADS_MONITOR_TASK.END_TIME is '任务结束时间';
COMMENT ON COLUMN  ADS_MONITOR_TASK.DEADLINE is '抽样信息上报截止时间';
COMMENT ON COLUMN  ADS_MONITOR_TASK.ATTACHMENT is '附件';
COMMENT ON COLUMN  ADS_MONITOR_TASK.ATTACHMENTCODE is '附件编号';
COMMENT ON COLUMN  ADS_MONITOR_TASK."COMMENT" is '备注';
COMMENT ON COLUMN  ADS_MONITOR_TASK.PUBLISH_STATUS is '发布状态';
COMMENT ON COLUMN  ADS_MONITOR_TASK.INDUSTRY is '行业';
COMMENT ON COLUMN  ADS_MONITOR_TASK.JUDGE_STANDARD is '判定标准';
COMMENT ON COLUMN  ADS_MONITOR_TASK.SAMPLE_LINK is '抽样环节';
COMMENT ON COLUMN  ADS_MONITOR_TASK.CHECK_OBJECT is '检测对象';
COMMENT ON COLUMN  ADS_MONITOR_TASK.CHECK_PROJECT is '检测项目';
COMMENT ON COLUMN  ADS_MONITOR_TASK.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_MONITOR_TASK.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_MONITOR_TASK.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_MONITOR_TASK.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_MONITOR_TASK.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_MONITOR_TASK.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_MONITOR_TASK.RESERVED_FIELD2 is '预留字段2';
COMMENT ON COLUMN  ADS_MONITOR_TASK.LEVE is '级别';
COMMENT ON COLUMN  ADS_MONITOR_TASK.ATTACHMENT_ADDRESS is '附件路径';
COMMENT ON COLUMN  ADS_MONITOR_TASK.ORGAN_ID is '组织机构ID';

alter table ADS_MONITOR_TASK ADD TASK_SOURCE VARCHAR2(32) DEFAULT 'DEPTASK';
COMMENT ON COLUMN  ADS_MONITOR_TASK.TASK_SOURCE is '任务来源';

COMMENT ON COLUMN  ADS_MONITOR_TASK.ABOLISH is '废止状态,(0代表未废止,1代表废止)';

-- -----------------------------
-- Table structure for 机构任务
-- -----------------------------
-- DROP TABLE ADS_ORGAN_TASK;
CREATE TABLE ADS_ORGAN_TASK (
  ID                 VARCHAR2(64) NOT NULL,
  MONITOR_TASK_ID    VARCHAR2(64) NOT NULL,
  SAMPLE_ORGAN       VARCHAR2(32) NOT NULL,
  SAMPLE_ORGAN_ID    VARCHAR2(64) NOT NULL,
  DETECTION_ORGAN    VARCHAR2(32) NOT NULL,
  DETECTION_ORGAN_ID VARCHAR2(64) NOT NULL,
  REG_ID             VARCHAR2(64),
  NUMBERS            NUMBER NOT NULL,
  DEPARMENT          VARCHAR2(32) NOT NULL,
  UPLOAD_TIME        TIMESTAMP NOT NULL,
  FINISH_NUM         NUMBER,
  REPORT_STATUS      VARCHAR2(32) NOT NULL,
  REPORT_TIME        TIMESTAMP NOT NULL,
  TASKS_STATUS       VARCHAR2(32) NOT NULL,
  SAMPLE_FINISH_NUM  NUMBER NOT NULL,
  CHECK_FINISH_NUM   NUMBER NOT NULL,
  SAMPLE_FINISH_STATUS NUMBER NOT NULL,
  CHECK_FINISH_STATUS NUMBER NOT NULL,
  SAMPLE_REPORT_STATUS NUMBER NOT NULL,
  CHECK_REPORT_STATUS NUMBER NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1) DEFAULT 'N',
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200),
  SAMPLE_REPORT_TIME TIMESTAMP,
  CHECK_REPORT_TIME TIMESTAMP
);

ALTER TABLE ADS_ORGAN_TASK ADD PRIMARY KEY (ID);

COMMENT ON TABLE ADS_ORGAN_TASK is '机构任务';
COMMENT ON COLUMN  ADS_ORGAN_TASK.ID is '主键id';
COMMENT ON COLUMN  ADS_ORGAN_TASK.MONITOR_TASK_ID is '监测任务KEY';
COMMENT ON COLUMN  ADS_ORGAN_TASK.SAMPLE_ORGAN is '抽样机构';
COMMENT ON COLUMN  ADS_ORGAN_TASK.SAMPLE_ORGAN_ID is '抽样机构KEY';
COMMENT ON COLUMN  ADS_ORGAN_TASK.DETECTION_ORGAN is '检测机构';
COMMENT ON COLUMN  ADS_ORGAN_TASK.DETECTION_ORGAN_ID is '检测机构KEY';
COMMENT ON COLUMN  ADS_ORGAN_TASK.REG_ID is '抽样所属行政区划';
COMMENT ON COLUMN  ADS_ORGAN_TASK.NUMBERS is '抽样数量';
COMMENT ON COLUMN  ADS_ORGAN_TASK.DEPARMENT is '报告上传单位';
COMMENT ON COLUMN  ADS_ORGAN_TASK.UPLOAD_TIME is '报告上传时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.FINISH_NUM is '完成数';
COMMENT ON COLUMN  ADS_ORGAN_TASK.REPORT_STATUS is '上报状态';
COMMENT ON COLUMN  ADS_ORGAN_TASK.REPORT_TIME is '上报时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.TASKS_STATUS is '任务状态';
COMMENT ON COLUMN  ADS_ORGAN_TASK.SAMPLE_FINISH_NUM is '抽样完成数';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CHECK_FINISH_NUM is '检测完成数';
COMMENT ON COLUMN  ADS_ORGAN_TASK.SAMPLE_FINISH_STATUS is '抽样完成状态';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CHECK_FINISH_STATUS is '检测完成状态';
COMMENT ON COLUMN  ADS_ORGAN_TASK.SAMPLE_REPORT_STATUS is '抽样上报状态';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CHECK_REPORT_STATUS is '检测上报状态';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_ORGAN_TASK.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_ORGAN_TASK.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_ORGAN_TASK.RESERVED_FIELD2 is '预留字段2';
COMMENT ON COLUMN  ADS_ORGAN_TASK.SAMPLE_REPORT_TIME is '抽样上报时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CHECK_REPORT_TIME is '检测上报时间';

alter table ads_organ_task add city_code varchar(40);
COMMENT ON COLUMN  ADS_ORGAN_TASK.city_code is '抽样省市区编码';

alter table ads_organ_task add city_name varchar(80);
COMMENT ON COLUMN  ADS_ORGAN_TASK.city_name is '抽样省市区域名称';

-- -----------------------------
-- Table structure for 监测抽样单
-- -----------------------------
-- DROP TABLE ADS_MONITOR_SAMPLE;
CREATE TABLE ADS_MONITOR_SAMPLE (
  ID         		VARCHAR2(64) NOT NULL,
  ORGAN_TASK_ID     VARCHAR2(64) NOT NULL,
  PRODUCT_TRACEABILITY_CODE VARCHAR2(64) NOT NULL,
  PRODUCT_CODE      VARCHAR2(64) NOT NULL,
  PRODUCT_NAME      VARCHAR2(64) NOT NULL,
  SAMPLE_BAR_CODE   NUMBER NOT NULL,
  SAMPLE_NAME       VARCHAR2(32) NOT NULL,
  SAMPLE_CODE       VARCHAR2(32) NOT NULL,
  TRADE_MARK        VARCHAR2(32) NOT NULL,
  PACKING           VARCHAR2(32),
  GRADE             VARCHAR2(32) NOT NULL,
  IDENTIFY          VARCHAR2(32),
  SPECIFICATION     VARCHAR2(32),
  STANDARD          VARCHAR2(32) NOT NULL,
  PRODUCE_DATE      TIMESTAMP NOT NULL,
  PRODUCE_CERTIFICATE VARCHAR2(1024),
  CERTIFICATE_CODE  VARCHAR2(128) NOT NULL,
  SAMPLE_BASE       NUMBER(8) NOT NULL,
  SAMPLE_PLACE      VARCHAR2(1024) NOT NULL,
  TESTED_DEPARMENT  VARCHAR2(32) NOT NULL,
  TESTED_ADDRESS    VARCHAR2(512) NOT NULL,
  TESTED_LEGALREP   VARCHAR2(32),
  TESTED_LINKMAN    VARCHAR2(32),
  TESTED_LINKMANPHONE VARCHAR2(16),
  TESTED_LINKMANFAX VARCHAR2(16),
  TESTED_PERSON     VARCHAR2(32) NOT NULL,
  TESTED_PERSONPHONE VARCHAR2(16),
  TESTED_PERSONFAX  VARCHAR2(16),
  PRODUCTIONS_TATUS VARCHAR2(32),
  PRODUCTION_DEPARMENT  VARCHAR2(32) NOT NULL,
  PRODUCTION_ADDRESS VARCHAR2(512) NOT NULL,
  PRODUCTION_ZIPCODE VARCHAR2(16),
  PRODUCTION_LINKMAN VARCHAR2(32),
  PRODUCTION_LINKMANPHONE   VARCHAR2(16),
  PRODUCTION_LINKMANFAX   VARCHAR2(16),
  SAMPLE_ID         VARCHAR2(64) NOT NULL,
  SAMPLE_DEPARMENT  VARCHAR2(32) NOT NULL,
  SAMPLE_LINKMAN    VARCHAR2(32) NOT NULL,
  SAMPLE_ADDRESS    VARCHAR2(512),
  SAMPLE_ZIPCODE    VARCHAR2(16),
  SAMPLE_PHONE      VARCHAR2(16),
  SAMPLE_FAX        VARCHAR2(16),
  SAMPLE_EMAIL      VARCHAR2(16),
  PROOF             VARCHAR2(1024),
  SAMPLE_PERSON     VARCHAR2(16) NOT NULL,
  SAMPLE_DATE       TIMESTAMP NOT NULL,
  SAMPLE_REPORT     VARCHAR2(32),
  "COMMENT"         VARCHAR2(1024),
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200),
  PRODUCING_AREA    VARCHAR2(1024)
  PRODUCING_AREA_NAME    VARCHAR2(1024)
);

ALTER TABLE ADS_MONITOR_SAMPLE ADD PRIMARY KEY (ID);
ALTER TABLE ADS_MONITOR_SAMPLE ADD UNIQUE(PRODUCT_TRACEABILITY_CODE);

COMMENT ON TABLE ADS_MONITOR_SAMPLE is '监测抽样单';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.ID is '主键id';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.ORGAN_TASK_ID is '机构任务KEY';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCT_TRACEABILITY_CODE is '产品追溯码';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCT_CODE is '产品编码';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCT_NAME is '产品名称';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_BAR_CODE is '抽样条码';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_NAME is '样品名称';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_CODE is '样品编号';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TRADE_MARK is '商标';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PACKING is '包装';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.GRADE is '等级';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.IDENTIFY is '标识';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SPECIFICATION is '型号规格';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.STANDARD is '执行标准';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCE_DATE is '生产日期';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCE_CERTIFICATE is '产品认证情况';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.CERTIFICATE_CODE is '证书编号';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_BASE is '抽样基数';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_PLACE is '抽样场所';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_DEPARMENT is '受检单位名称';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_ADDRESS is '受检单位地址';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_LEGALREP is '受检单位法人代表';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_LINKMAN is '受检单位联系人';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_LINKMANPHONE is '受检单位联系人电话';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_LINKMANFAX is '受检单位联系人电话传真';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_PERSON is '受检人';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_PERSONPHONE is '受检人电话';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.TESTED_PERSONFAX is '受检人传真';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTIONS_TATUS is '生产单位状态';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTION_DEPARMENT is '生产单位名称';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTION_ADDRESS is '生产单位地址';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTION_ZIPCODE is '生产单位邮编';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTION_LINKMAN is '生产单位联系人';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTION_LINKMANPHONE is '生产单位联系人电话';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PRODUCTION_LINKMANFAX is '生产单位联系人传真';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_ID  is '抽样单位KEY';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_DEPARMENT  is '抽样单位名称';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_LINKMAN  is '抽样单位联系人';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_ADDRESS  is '抽样单位地址';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_ZIPCODE  is '抽样单位邮编';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_PHONE  is '抽样单位电话';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_FAX  is '抽样单位传真';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_EMAIL  is '抽样单位Email';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.PROOF  is '检测任务依据';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_PERSON  is '抽样人';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_DATE  is '抽样日期';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE.SAMPLE_REPORT  is '上报状态';
COMMENT ON COLUMN  ADS_MONITOR_SAMPLE."COMMENT"  is '备注';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_ORGAN_TASK.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_ORGAN_TASK.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_ORGAN_TASK.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_ORGAN_TASK.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_ORGAN_TASK.RESERVED_FIELD2 is '预留字段2';
COMMENT ON COLUMN  ADS_ORGAN_TASK.PRODUCING_AREA is '产地';
COMMENT ON COLUMN  ADS_ORGAN_TASK.PRODUCING_AREA_NAME is '产地名称';

-- ------------------------------
-- Table structure for 检测信息表
-- ------------------------------
-- DROP TABLE ADS_CHECK_INFO;
CREATE TABLE ADS_CHECK_INFO (
  ID         		 VARCHAR2(64) NOT NULL,
  SAMPLE_BAR_CODE    VARCHAR2(64) NOT NULL,
  MONITOR_SAMPLE_ID  VARCHAR2(64) NOT NULL,
  PRODUCT_TRACEABILITY_CODE VARCHAR2(64) NOT NULL,
  PRODUCT_CODE       VARCHAR2(32) NOT NULL,
  PRODUCT_NAME       VARCHAR2(64) NOT NULL,
  SAMPLE_CODE        VARCHAR2(32) NOT NULL,
  SAMPLE_NAME        VARCHAR2(32) NOT NULL,
  CHECK_LINK         VARCHAR2(32) NOT NULL,
  RESULT             VARCHAR2(2) NOT NULL,
  CHECK_ORGAN_ID     VARCHAR2(64) NOT NULL,
  CHECK_ORGAN        VARCHAR2(32) NOT NULL,
  CHECK_REPORT       VARCHAR2(2) NOT NULL,
  CHECK_TIME         TIMESTAMP NOT NULL,
  CREATE_BY          VARCHAR2(64) NOT NULL,
  CREATE_TIME        TIMESTAMP NOT NULL,
  UPDATE_BY          VARCHAR2(64),
  UPDATE_TIME        TIMESTAMP,
  DEL_FLAG           VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200),
  TESTED_DEPARMENT  VARCHAR2(64),
  SAMPLE_DEPARMENT  VARCHAR2(64)
);

ALTER TABLE ADS_CHECK_INFO ADD PRIMARY KEY (ID);
ALTER TABLE ADS_CHECK_INFO ADD UNIQUE(PRODUCT_TRACEABILITY_CODE);
CREATE INDEX SO_SAMPLE_BAR_IDX ON ADS_CHECK_INFO(SAMPLE_BAR_CODE);
CREATE INDEX SO_MONITOR_SAMPLE_ID_IDX ON ADS_CHECK_INFO(MONITOR_SAMPLE_ID);

COMMENT ON TABLE ADS_CHECK_INFO is '检测信息表';
COMMENT ON COLUMN  ADS_CHECK_INFO.ID is '主键id';
COMMENT ON COLUMN  ADS_CHECK_INFO.SAMPLE_BAR_CODE is '抽样条码';
COMMENT ON COLUMN  ADS_CHECK_INFO.MONITOR_SAMPLE_ID is '抽样单KEY';
COMMENT ON COLUMN  ADS_CHECK_INFO.PRODUCT_TRACEABILITY_CODE is '产品追溯码';
COMMENT ON COLUMN  ADS_CHECK_INFO.PRODUCT_CODE is '产品编码';
COMMENT ON COLUMN  ADS_CHECK_INFO.PRODUCT_NAME is '产品名称';
COMMENT ON COLUMN  ADS_CHECK_INFO.SAMPLE_CODE is '样品编码';
COMMENT ON COLUMN  ADS_CHECK_INFO.SAMPLE_NAME is '样品名称';
COMMENT ON COLUMN  ADS_CHECK_INFO.CHECK_LINK is '检测环节';
COMMENT ON COLUMN  ADS_CHECK_INFO.RESULT is '判定结果';
COMMENT ON COLUMN  ADS_CHECK_INFO.CHECK_ORGAN_ID is '检测单位KEY';
COMMENT ON COLUMN  ADS_CHECK_INFO.CHECK_ORGAN is '检测单位';
COMMENT ON COLUMN  ADS_CHECK_INFO.CHECK_REPORT is '检测上传状态';
COMMENT ON COLUMN  ADS_CHECK_INFO.CHECK_TIME is '检测时间';
COMMENT ON COLUMN  ADS_CHECK_INFO.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_CHECK_INFO.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_CHECK_INFO.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_CHECK_INFO.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_CHECK_INFO.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_CHECK_INFO.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_CHECK_INFO.RESERVED_FIELD2 is '预留字段2';
COMMENT ON COLUMN  ADS_CHECK_INFO.TESTED_DEPARMENT is '受检单位';
COMMENT ON COLUMN  ADS_CHECK_INFO.SAMPLE_DEPARMENT is '抽样单位';

-- ------------------------------
-- Table structure for 检测项目
-- ------------------------------
-- DROP TABLE ADS_CHECK_PROJECT;
CREATE TABLE ADS_CHECK_PROJECT (
  ID         		VARCHAR2(64) NOT NULL,
  CHECK_PROJECT     VARCHAR2(32) NOT NULL,
  CHECK_NUM         VARCHAR2(32) NOT NULL,
  JUDGE_NUM         VARCHAR2(32) NOT NULL,
  RESULT            VARCHAR2(32) NOT NULL,
  CHECK_INFO_ID     VARCHAR2(64) NOT NULL,
  CREATE_BY         VARCHAR2(64) NOT NULL,
  CREATE_TIME       TIMESTAMP NOT NULL,
  UPDATE_BY         VARCHAR2(64),
  UPDATE_TIME       TIMESTAMP,
  DEL_FLAG          VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200)
);

ALTER TABLE ADS_CHECK_PROJECT ADD PRIMARY KEY (ID);
ALTER TABLE ADS_CHECK_PROJECT ADD CONSTRAINT CHECK_INFO_ID_CONS FOREIGN KEY (CHECK_INFO_ID) REFERENCES ADS_CHECK_INFO;

COMMENT ON TABLE ADS_CHECK_PROJECT is '检测项目';
COMMENT ON COLUMN  ADS_CHECK_PROJECT.ID is '主键id';
COMMENT ON COLUMN  ADS_CHECK_PROJECT.CHECK_PROJECT is '检测项目名称';
COMMENT ON COLUMN  ADS_CHECK_PROJECT.CHECK_NUM is '检出值';
COMMENT ON COLUMN  ADS_CHECK_PROJECT.JUDGE_NUM is '判定值';
COMMENT ON COLUMN  ADS_CHECK_PROJECT.RESULT is '抽检结果';
COMMENT ON COLUMN  ADS_CHECK_PROJECT.CHECK_INFO_ID is '检测信息Key';
COMMENT ON COLUMN  ADS_CHECK_INFO.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_CHECK_INFO.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_CHECK_INFO.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_CHECK_INFO.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_CHECK_INFO.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_CHECK_INFO.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_CHECK_INFO.RESERVED_FIELD2 is '预留字段2';


ALTER TABLE ADS_CHECK_INFO ADD ORGAN_TASK_ID VARCHAR2(64) NOT NULL;
COMMENT ON COLUMN  ADS_CHECK_INFO.ORGAN_TASK_ID is '机构任务KEY';
ALTER TABLE ADS_CHECK_INFO ADD CONSTRAINT ORGAN_TASK_ID_CHECK FOREIGN KEY (ORGAN_TASK_ID) REFERENCES ADS_ORGAN_TASK;


-- -----------------------------
-- Table structure for 检测信息_检测项目
-- -----------------------------
-- DROP TABLE ADS_INFO_PROJECT;
CREATE TABLE ADS_INFO_PROJECT (
  ID                 VARCHAR2(64) NOT NULL,
  CHECK_PROJECT       VARCHAR2(32) NOT NULL,
  CHECK_NUM           VARCHAR2(32) NOT NULL,
  JUDGE_NUM         VARCHAR2(32) NOT NULL,
  RESULT            VARCHAR2(2) NOT NULL,
  CHECK_INFO_ID     VARCHAR2(64) NOT NULL,
  CREATE_BY          VARCHAR2(64) NOT NULL,
  CREATE_TIME        TIMESTAMP NOT NULL,
  UPDATE_BY          VARCHAR2(64),
  UPDATE_TIME        TIMESTAMP,
  DEL_FLAG           VARCHAR2(1),
  RESERVED_FIELD1   VARCHAR2(200),
  RESERVED_FIELD2   VARCHAR2(200)
);

ALTER TABLE ADS_INFO_PROJECT ADD PRIMARY KEY (ID);
ALTER TABLE ADS_INFO_PROJECT ADD CONSTRAINT CHECK_CHECK_INFO_ID_CONS FOREIGN KEY (CHECK_INFO_ID) REFERENCES ADS_CHECK_INFO;
ALTER TABLE ADS_INFO_PROJECT ADD REQUIRED VARCHAR2(32) DEFAULT '1';

COMMENT ON TABLE ADS_INFO_PROJECT is '监测任务_检测项目';
COMMENT ON COLUMN  ADS_INFO_PROJECT.ID is '主键id';
COMMENT ON COLUMN  ADS_INFO_PROJECT.CHECK_PROJECT is '检测项目';
COMMENT ON COLUMN  ADS_INFO_PROJECT.CHECK_NUM is '检出值';
COMMENT ON COLUMN  ADS_INFO_PROJECT.JUDGE_NUM is '判定值';
COMMENT ON COLUMN  ADS_INFO_PROJECT.RESULT is '抽检结果';
COMMENT ON COLUMN  ADS_INFO_PROJECT.CHECK_INFO_ID is '检测信息Key';
COMMENT ON COLUMN  ADS_INFO_PROJECT.CREATE_BY is '创建人ID';
COMMENT ON COLUMN  ADS_INFO_PROJECT.CREATE_TIME is '创建时间';
COMMENT ON COLUMN  ADS_INFO_PROJECT.UPDATE_BY is '修改人ID';
COMMENT ON COLUMN  ADS_INFO_PROJECT.UPDATE_TIME is '修改时间';
COMMENT ON COLUMN  ADS_INFO_PROJECT.DEL_FLAG is '删除标识';
COMMENT ON COLUMN  ADS_INFO_PROJECT.RESERVED_FIELD1 is '预留字段1';
COMMENT ON COLUMN  ADS_INFO_PROJECT.RESERVED_FIELD2 is '预留字段2';
COMMENT ON COLUMN  ADS_INFO_PROJECT.REQUIRED is '必填';